@book{fuchs,
	Abstract = {This book gives a well rounded yet in depth study of virtual reality, specifically the relationship between human beings and virtual environments. This source has a great introduction that covers many of the crucial definitions one should be familiar with when referencing virtual reality. The work then moves on to cover human behaviors in real world environments and how we can begin to analyze and model that within a virtual system. It breaks down the major human senses used in virtual reality and gives biological explanations on how each of these systems function and operate. Another relevant chapter this book offers is an in depth discussion of immersion and presence within virtual reality. This source is written for people well versed in the concepts of virtual reality and looking to dig deeper into the role human senses play in the relationship between human beings and virtual environments. This is a fantastic source for my paper because it can be referenced to write about the human senses, and why they must be understood to model effective virtual reality.},
	Author = {Fuchs, Philippe and Moreau, Guillaume and Guitton, Pascal},
	Date-Modified = {2016-11-13 22:39:00 +0000},
